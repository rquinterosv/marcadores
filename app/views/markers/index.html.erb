<p id="notice"><%= notice %></p>

<h1>Markers</h1>

<div id="marker-form-container">
  <%= render 'form', marker: @marker %>
</div>

<div id="marker-complete">
</div>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
      <th>State</th>
      <th>Type</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="marker">
  <%= render @markers%>
  </tbody>
</table>

<br>


<%= pie_chart Marker.group(:type_id).count %>
